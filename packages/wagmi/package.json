{
  "name": "@sky-mavis/tanto-wagmi",
  "type": "module",
  "version": "0.0.10",
  "description": "Tanto Wagmi",
  "main": "dist/cjs/index.cjs",
  "module": "dist/mjs/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "gen-version": "genversion --semi --esm --property name,version src/version.ts",
    "build": "rm -rf dist && yarn gen-version && rollup -c",
    "dev": "rollup -c -w",
    "lint": "node ./node_modules/eslint/bin/eslint ../.. --ext .js --fix",
    "lint:fix": "eslint ../.. --ext .js,.tsx,.ts --fix",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:skymavis/tanto-kit.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/mjs/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/mjs/index.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "keywords": [
    "Ronin",
    "Waypoint",
    "Tanto",
    "Tanto Connect",
    "Tanto Kit",
    "Ronin Dapps",
    "Ronin Wallet",
    "Ronin Network",
    "Ronin Waypoint",
    "Ronin Wallet SDK",
    "Ronin Provider",
    "Ronin Connect",
    "Ronin Wagmi Connector"
  ],
  "author": "SkyMavis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skymavis/tanto-kit/issues"
  },
  "homepage": "https://github.com/skymavis/tanto-kit",
  "dependencies": {
    "@sky-mavis/tanto-connect": "0.0.20"
  },
  "peerDependencies": {
    "@wagmi/core": "^2.x",
    "viem": "^2.x"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-json": "6.1.0",
    "@types/jest": "29.5.11",
    "@types/react": "18.0.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "genversion": "3.2.0",
    "jest": "29.7.0",
    "prettier": "2.8.1",
    "rollup": "3.29.4",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.5"
  }
}
